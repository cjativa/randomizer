{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "noEmitOnError": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "../dist/",
    "baseUrl": "../server",
    "esModuleInterop": true,
    "strict": false
  },
  "compileOnSave": true,
  "include": ["**/*.ts"],
  "exclude": ["src"]
}
